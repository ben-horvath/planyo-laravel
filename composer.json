{
    "name": "ben-horvath/planyo-laravel",
    "description": "A Laravel package to handle the Planyo API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Horvath",
            "email": "ben@meetbenhorvath.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "donatj/mock-webserver": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "benhorvath\\PlanyoLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "benhorvath\\PlanyoLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "benhorvath\\PlanyoLaravel\\PlanyoLaravelServiceProvider"
            ]
        }
    }
}
